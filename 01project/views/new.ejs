<%- include("partials/header") %>
    <div class="container">
        <a class="btn btn-primary btn-xs" href="/visited">Back</a>
        <div class="row">
            <div style="width: 30%; margin: 25px auto;">
                <form action="/visited" method="POST">
                    <h4 style="text-align: center;">Add Details about the place where you have visited</h4>
                    <div class="form-group">
                        <label for="city">City</label>
                        <input class="form-control" id="city" type="text" placeholder="City" name="name">
                    </div>
                    <div class="form-group">
                        <label for="Photo">Photo URL</label>
                        <input class="form-control" id="photo" type="text" placeholder="Imageurl" name="image">
                    </div>
                    <div class="form-group">
                        <label for="comment">Few words about this place</label>
                        <textarea class="form-control" rows="5" id="comment" name="about"></textarea>
                    </div>
                    <div class="form-group form-check">
                        <input type="checkbox" class="form-check-input" id="Check1">
                        <label class="form-check-label" for="Check1">VISITED</label>
                        <input type="checkbox" class="form-check-input" id="Check2">
                        <label class="form-check-label" for="Check2">BUCKET-LIST</label>
                    </div>

                    <div class="form-group text-right ">
                        <button class="btn btn-xs btn-primary ">ADD PLACE</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <%- include("partials/footer") %>